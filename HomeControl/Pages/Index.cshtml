@page
@using HomeControl.Integrations
@model IndexModel
@{
	ViewData["Title"] = "Home";
}

<div class="grid body-content">
	@{
		foreach (var menuItem in Helpers.MainMenuHelper.MenuItems)
		{
			if (menuItem.Name == "Home") continue;

			<card-a class="card card-padding" asp-page="@menuItem.Url">
				<card-element header="@menuItem.Name"></card-element>
			</card-a>
		}
	}
</div>